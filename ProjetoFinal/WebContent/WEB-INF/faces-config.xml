<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
<managed-bean>
		<managed-bean-name>cadUser</managed-bean-name>
		<managed-bean-class>com.betha.beans.CadastroUserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>redirecionarPagina</managed-bean-name>
		<managed-bean-class>com.betha.beans.RedirecionarPaginaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>layoutBean</managed-bean-name>
		<managed-bean-class>com.betha.beans.LayoutBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>relatorioBean</managed-bean-name>
		<managed-bean-class>com.betha.beans.RelatorioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>selecionadoBean</managed-bean-name>
		<managed-bean-class>com.betha.beans.SelecionarBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
